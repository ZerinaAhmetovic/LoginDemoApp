<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- The drawable object that manages the array of Apple type RatingBar drawables. -->
    <item
        android:id="@android:id/background"
        android:drawable="@drawable/rate_dialog_star_unfilled"/> <!-- unfilled -->
    <!-- @android:id/secondaryProgress should not be modified by the client as it is used internally
         as the background for a fractionally filled star. -->
    <item
        android:id="@android:id/progress"
        android:drawable="@drawable/rate_dialog_star_filled"/>   <!-- filled -->

</layer-list>